psi.affordableApplicationApproval = {};

psi.affordableApplicationApproval = ( function() {

	var approveAffordableApplication = (function( intapplicationId ) {

		psi.patterns.loadLargeDialog( {
			strUrl: '/?module=application_systemxxx&application[id]=' + intapplicationId,
			onCloseCallback: function() {
				psi.objDashboard.reloadCurrentTab( 'dashboard_applications_table' );
			}
		} );

	});

	return {
		approveAffordableApplication:approveAffordableApplication
	};

})( jQuery );