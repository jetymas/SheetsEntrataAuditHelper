		if( '' !== strErrorMsgs ) {
			$('.global-error').html( '<span class="alert slim error"><i></i>' + strErrorMsgs + '</span>').fadeIn(100);
			$( '.alert_display_options_error_div' ).html( '<p class="alert error slim"><i></i>' + strErrorMsgs + '</p>' ).show();
			return false;
		}

		return true;
	}

