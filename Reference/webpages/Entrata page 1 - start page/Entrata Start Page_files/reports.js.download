		// Report sorting generation callbacks.
		$(document).off( 'click', 'thead .sortable' ).on( 'click', 'thead .sortable', function( e ) {
			e.preventDefault();

			var viewDataContainer = $(this).parents('.view-data-container'),
				sortBy = $(this).attr('data-sort'),
